{
  "name": "portfolio",
  "version": "1.0.0",
  "description": "This is my portfolio.",
  "main": "index.html",
  "scripts": {
    "start": "npm run watch:all",
    "scss": "node-sass --watch css/ -o css/",
    "lite": "lite-server",
    "watch:all": "parallelshell \"npm run scss\" \"npm run lite\"",
    "clean": "rimraf dist",
    "copyfonts": "copyfiles -f node_modules/font-awesome/fonts/* dist/fonts",
    "imagemin": "imagemin img/* --out-dir dist/img",
    "usemin": "usemin index.html -d dist -o dist/index.html",
    "build": "npm run clean && npm run copyfonts && npm run imagemin && npm run usemin"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jesusriosdev/portfolio.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/jesusriosdev/portfolio/issues"
  },
  "homepage": "https://github.com/jesusriosdev/portfolio#readme",
  "devDependencies": {
    "copyfiles": "^2.2.0",
    "cssmin": "^0.4.3",
    "del": "^3.0.0",
    "gulp": "^4.0.0",
    "gulp-clean-css": "^3.9.3",
    "gulp-flatmap": "^1.0.2",
    "gulp-htmlmin": "^4.0.0",
    "gulp-imagemin": "^6.2.0",
    "gulp-rev": "^8.1.1",
    "gulp-sass": "^3.1.0",
    "gulp-uglify": "^3.0.0",
    "gulp-uglifycss": "^1.1.0",
    "gulp-usemin": "^0.3.29",
    "htmlmin": "0.0.7",
    "imagemin": "^7.0.1",
    "imagemin-cli": "^6.0.0",
    "lite-server": "^2.5.4",
    "node-sass": "^7.0.0",
    "onchange": "^6.1.0",
    "parallelshell": "^3.0.2",
    "rimraf": "^2.7.1",
    "uglifyjs": "^2.4.11",
    "usemin-cli": "^0.6.0"
  },
  "dependencies": {
    "bootstrap": "^4.4.1",
    "font-awesome": "^4.7.0",
    "jquery": "^3.5.0",
    "popper.js": "^1.16.1"
  }
}
